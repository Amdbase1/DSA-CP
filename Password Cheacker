#include <bits/stdc++.h>
using namespace std;

bool strongPasswordCheckerII(string password) {
          int l1=0,u=0,d=0,s=0,k=1;
  int l=password.size();
  if(l>=8){
     for(int i=0;i<password.size()-1;i++){ 
         if(password[i]==password[i+1])
         return false;
     }
  for(int i=0;i<password.size();i++){
   if(password[i]>='a' && password[i]<='z')
  l1=1;
  else if(password[i]>='A'&& password[i]<='Z')
  u=1;
  else if(password[i]>='0' && password[i]<='9')
  d=1;
  else if(password[i]=='!' || password[i]=='@' || password[i]=='#'|| password[i]=='$'|| password[i]=='%'|| password[i]=='^'|| password[i]=='&' || password[i]=='*'|| password[i]=='('||password[i]==')' ||password[i]=='-' || password[i]=='+')
  
      s=1;
      else
      k=0;
  }
  if(l1==1 && s==1 && d==1 && u==1 && k==1)
  return true;
  else
  return false;
  }
  return false;
    }
    int main(){
    string p;
    getline(cin,p);
    cout<<strongPasswordCheckerII;
    return 0;
    }
    
